{
  "name": "vuex-workshop",
  "version": "1.0.0",
  "description": "A Vuex workshop",
  "author": "Blake Newman <blake.newman@askattest.com>",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env BABEL_ENV=client NODE_ENV=production webpack --config config/webpack.client.js --progress --hide-modules",
    "build:server": "cross-env BABEL_ENV=server NODE_ENV=production webpack --config config/webpack.server.js --progress --hide-modules",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "eslint --ignore-path .gitignore --ext .js,.vue .",
    "stylelint": "stylelint 'src/**/*(css|vue|html)' --config ./.stylelintrc.js"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "axios": "0.16.2",
    "eslint-plugin-import": "^2.3.0",
    "normalize.css": "7.0.0",
    "reset.css": "2.0.2",
    "vue": "2.3.4",
    "vue-router": "2.5.3",
    "vuex": "2.3.1",
    "vuex-router-sync": "4.2.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-stage-2": "6.24.1",
    "body-parser": "1.17.2",
    "compression": "1.6.2",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "eslint": "3.9.1",
    "eslint-config-vue": "2.0.2",
    "eslint-plugin-vue": "2.0.1",
    "express": "4.15.3",
    "file-loader": "0.11.2",
    "friendly-errors-webpack-plugin": "1.6.1",
    "lru-cache": "4.1.1",
    "rimraf": "2.6.1",
    "spdy": "3.4.7",
    "stylelint": "7.11.1",
    "stylelint-config-standard": "16.0.0",
    "stylelint-processor-html": "1.0.0",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "vue-loader": "12.2.1",
    "vue-server-renderer": "2.3.4",
    "vue-style-loader": "3.0.1",
    "vue-template-compiler": "2.3.4",
    "webpack": "2.6.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
